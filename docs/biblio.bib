@article{groff_simulation_2007,
	title = {Simulation for Theory Testing and Experimentation: An Example Using Routine Activity Theory and Street Robbery},
	volume = {23},
	issn = {1573-7799},
	url = {https://doi.org/10.1007/s10940-006-9021-z},
	doi = {10.1007/s10940-006-9021-z},
	shorttitle = {Simulation for Theory Testing and Experimentation},
	abstract = {Achieving a better understanding of the crime event in its spatio-temporal context is an important research area in criminology with major implications for improving policy and developing effective crime prevention strategies. However, significant barriers related to data and methods exist for conducting this type of research. The research requires micro-level data about individual behavior that is difficult to obtain and methods capable of modeling the dynamic, spatio-temporal interaction of offenders, victims, and potential guardians at the micro level. This paper presents simulation modeling as a method for addressing these challenges. Specifically, agent-based modeling, when integrated with geographic information systems, offers the ability to model individual behavior within a real environment. The method is demonstrated by operationalizing and testing routine activity theory as it applies to the crime of street robbery. Model results indicate strong support for the basic premise of routine activity theory; as time spent away from home increases, crime will increase. The strength of the method is in providing a research platform for translating theory into models that can be discussed, shared, tested and enhanced with the goal of building scientific knowledge.},
	pages = {75--103},
	number = {2},
	journaltitle = {Journal of Quantitative Criminology},
	shortjournal = {J Quant Criminol},
	author = {Groff, Elizabeth R.},
	urldate = {2019-08-21},
	date = {2007-06-01},
	langid = {english},
	keywords = {Agent-based models, Experiment, Geographic information systems, Simulation, Theory testing}
}

@article{chen_crime_2004,
	title = {Crime data mining: a general framework and some examples},
	volume = {37},
	issn = {0018-9162},
	doi = {10.1109/MC.2004.1297301},
	shorttitle = {Crime data mining},
	abstract = {A major challenge facing all law-enforcement and intelligence-gathering organizations is accurately and efficiently analyzing the growing volumes of crime data. Detecting cybercrime can likewise be difficult because busy network traffic and frequent online transactions generate large amounts of data, only a small portion of which relates to illegal activities. Data mining is a powerful tool that enables criminal investigators who may lack extensive training as data analysts to explore large databases quickly and efficiently. We present a general framework for crime data mining that draws on experience gained with the Coplink project, which researchers at the University of Arizona have been conducting in collaboration with the Tucson and Phoenix police departments since 1997.},
	pages = {50--56},
	number = {4},
	journaltitle = {Computer},
	author = {Chen, H. and Chung, W. and Xu, J. J. and Wang, G. and Qin, Y. and Chau, M.},
	date = {2004-04},
	keywords = {crime data mining, data mining, Cities and towns, computer crime, Computer crime, Costs, cybercrime detection, Data mining, Data security, intelligence-gathering organizations, law administration, law-enforcement organizations, Local government, Monitoring, National security, network traffic, online transactions, Pattern analysis, police data processing, Terrorism}
}

