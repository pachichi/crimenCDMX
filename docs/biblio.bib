	title = {A random walk in the literature on criminality: A partial and critical view on some statistical analyses and modelling approaches},
	volume = {21},
	issn = {1469-4425, 0956-7925},
	url = {https://www.cambridge.org/core/journals/european-journal-of-applied-mathematics/article/random-walk-in-the-literature-on-criminality-a-partial-and-critical-view-on-some-statistical-analyses-and-modelling-approaches/C42B0CD1851A7FE3DF58FD658E55FDC9},
